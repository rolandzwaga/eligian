{
  "name": "@eligian/compiler",
  "version": "0.0.1",
  "description": "Effect-based compiler for the Eligian DSL",
  "license": "ISC",
  "author": "",
  "type": "module",
  "main": "./out/index.js",
  "module": "./out/index.js",
  "exports": {
    ".": {
      "types": "./out/index.d.ts",
      "default": "./out/index.js"
    }
  },
  "typesVersions": {
    "*": {
      ".": [
        "out/index"
      ]
    }
  },
  "files": [
    "out",
    "src"
  ],
  "engines": {
    "node": ">=20.10.0",
    "npm": ">=10.2.3"
  },
  "scripts": {
    "clean": "shx rm -fr *.tsbuildinfo out",
    "build": "echo 'No build step'",
    "build:clean": "pnpm run clean && pnpm run build",
    "test": "vitest run",
    "generate:registry": "tsx src/operations/generate-registry.ts"
  },
  "dependencies": {
    "effect": "3.12.0",
    "eligius": "1.1.5",
    "fast-check": "^3.23.2"
  },
  "devDependencies": {
    "tsx": "4.19.2",
    "vitest": "3.2.4"
  }
}
